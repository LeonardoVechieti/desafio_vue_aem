<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Properties"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <buttons
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Bot達o"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
                        <items 
                            jcr:primaryType="nt:unstructured">
                            <textButton
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Texto do Bot達o"
                                name="./textButton"/>
                            <fontButton 
                                cq-msm-lockable="./fontButton"
                                defaultValue="Space Mono"
                                fieldLabel="Selecione a fonte do bot達o:"
                                jcr:primaryType="nt:unstructured"
                                name="./fontButton"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                value="false">
                                <items jcr:primaryType="nt:unstructured">
                                        <SpaceMono jcr:primaryType="nt:unstructured" text="SpaceMono" value="Space Mono"/>
                                        <Inconsolata jcr:primaryType="nt:unstructured" text="Inconsolata" value="Inconsolata"/>
                                        <Montserrat jcr:primaryType="nt:unstructured" text="Montserrat" value="Montserrat"/>      
                                </items>
                            </fontButton> 
                            <textColor
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"
                                fieldLabel="Cor do texto"
                                showDefaultColors="false"
                                name="./textColor">
                                <items
                                    jcr:primaryType="nt:unstructured">
                                    <preto
                                        jcr:primaryType="nt:unstructured"
                                        value="#000000"/>
                                    <azul
                                        jcr:primaryType="nt:unstructured"
                                        value="#0000FF"/>
                                    <verde
                                        jcr:primaryType="nt:unstructured"
                                        value="#00FF00"/>
                                    <vermelho
                                        jcr:primaryType="nt:unstructured"
                                        value="#FF0000"/>
                                    <cinza
                                        jcr:primaryType="nt:unstructured"
                                        value="#898686"/>
                                    <branco
                                        jcr:primaryType="nt:unstructured"
                                        value="#FFFFFF"/>
                                    <rosa
                                        jcr:primaryType="nt:unstructured"
                                        value="#FF00FF"/>
                                    <laranja
                                        jcr:primaryType="nt:unstructured"
                                        value="#FFA500"/>
                                    <roxo 
                                        jcr:primaryType="nt:unstructured"
                                        value="#800080"/>
                                    <marrom 
                                        jcr:primaryType="nt:unstructured"
                                        value="#8B4513"/>
                                    <azul-claro
                                        jcr:primaryType="nt:unstructured"
                                        value="#00FFFF"/>
                                    <verde-claro
                                        jcr:primaryType="nt:unstructured"
                                        value="#00FF00"/>
                                    <vermelho-claro
                                        jcr:primaryType="nt:unstructured"
                                        value="#FF0000"/>
                                    <cinza-claro
                                        jcr:primaryType="nt:unstructured"
                                        value="#C0C0C0"/>
                                    <laranja-claro
                                        jcr:primaryType="nt:unstructured"
                                        value="#e6d218"/>
                                </items>
                            </textColor>
                            <colorButton
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"
                                fieldLabel="Cor do bot達o"
                                showDefaultColors="false"
                                name="./colorButton">
                                <items
                                    jcr:primaryType="nt:unstructured">
                                    <preto
                                        jcr:primaryType="nt:unstructured"
                                        value="#000000"/>
                                    <azul
                                        jcr:primaryType="nt:unstructured"
                                        value="#0000FF"/>
                                    <verde
                                        jcr:primaryType="nt:unstructured"
                                        value="#00FF00"/>
                                    <vermelho
                                        jcr:primaryType="nt:unstructured"
                                        value="#FF0000"/>
                                    <cinza
                                        jcr:primaryType="nt:unstructured"
                                        value="#898686"/>
                                    <branco
                                        jcr:primaryType="nt:unstructured"
                                        value="#FFFFFF"/>
                                    <rosa
                                        jcr:primaryType="nt:unstructured"
                                        value="#FF00FF"/>
                                    <laranja
                                        jcr:primaryType="nt:unstructured"
                                        value="#FFA500"/>
                                    <roxo 
                                        jcr:primaryType="nt:unstructured"
                                        value="#800080"/>
                                    <marrom 
                                        jcr:primaryType="nt:unstructured"
                                        value="#8B4513"/>
                                    <azul-claro
                                        jcr:primaryType="nt:unstructured"
                                        value="#00FFFF"/>
                                    <verde-claro
                                        jcr:primaryType="nt:unstructured"
                                        value="#00FF00"/>
                                    <vermelho-claro
                                        jcr:primaryType="nt:unstructured"
                                        value="#FF0000"/>
                                    <cinza-claro
                                        jcr:primaryType="nt:unstructured"
                                        value="#C0C0C0"/>
                                    <laranja-claro
                                        jcr:primaryType="nt:unstructured"
                                        value="#e6d218"/>
                                </items>
                            </colorButton>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
